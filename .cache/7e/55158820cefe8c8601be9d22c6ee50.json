n  src: url(\"../fonts/Poppins/Poppins-Black.ttf\");\r\n  font-weight: 900;\r\n  font-display: swap;\r\n}\r\n@font-face {\r\n  font-family: \"Poppins\";\r\n  src: url(\"../fonts/Poppins/Poppins-BlackItalic.ttf\");\r\n  font-weight: 900;\r\n  font-style: italic;\r\n  font-display: swap;\r\n}\r\n\r\n// Paleta colores\r\n$primario: #4c8d26;\r\n$secundario: #d5fb00;\r\n$complementario: #882380;\r\n$dark: #223c20;\r\n$light: #f8fafc;\r\n\r\n// Breakpoints\r\n$small: 480px;\r\n$medium: 768px;\r\n$large: 1024px;\r\n$extra-large: 1600px;\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n  text-decoration: none;\r\n  list-style: none;\r\n}\r\n\r\nhtml {\r\n  font-size: 62.5%;\r\n  font-family: \"Poppins\", sans-serif;\r\n}\r\n\r\n.container {\r\n  width: 100%;\r\n  padding: 0 2rem;\r\n}\r\n","scss/layouts/_header.scss":"@use \"./../customs/globals\" as *;\r\n\r\n.header {\r\n  background-image: url(\"../img/hero_landscape.jpg\");\r\n  background-position: center center;\r\n  background-size: cover;\r\n  position: relative;\r\n  height: 100vh;\r\n  width: 100vw;\r\n\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n\r\n  &__title {\r\n    height: 100vh;\r\n    margin: 20vh 0;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n  }\r\n\r\n  &__logo {\r\n    color: $light;\r\n    font-size: 4rem;\r\n\r\n    &--text {\r\n      color: $light;\r\n    }\r\n  }\r\n\r\n  &__heading {\r\n    color: $secundario;\r\n    font-size: 4rem;\r\n    text-shadow: 0.2rem 0.2rem 0.2rem lighten($color: $secundario, $amount: 25%);\r\n\r\n    @media (min-width: $medium) {\r\n      font-size: 4.8rem;\r\n    }\r\n    @media (min-width: $large) {\r\n      font-size: 6rem;\r\n      width: 75%;\r\n    }\r\n  }\r\n}\r\n","scss/layouts/_navbar.scss":"@use \"./../customs/globals\" as *;\r\n\r\n.navbar {\r\n  height: 100vh;\r\n  width: 100vw;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n\r\n  &__hamburguer {\r\n    width: 5rem;\r\n    height: 5rem;\r\n    position: fixed;\r\n    z-index: 99;\r\n    top: 3rem;\r\n    left: 3rem;\r\n    background-color: $complementario;\r\n    cursor: pointer;\r\n    border: 0.3rem outset $secundario;\r\n    border-radius: 50%;\r\n\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  &__cover {\r\n    background: linear-gradient(180deg, $primario 25%, $complementario 100%);\r\n    width: 1rem;\r\n    height: 1rem;\r\n    position: fixed;\r\n    top: 6rem;\r\n    left: 6rem;\r\n    border-radius: 50%;\r\n    transition: transform 0.3s;\r\n  }\r\n\r\n  &__list {\r\n    width: inherit;\r\n    height: inherit;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    opacity: 0;\r\n    visibility: hidden;\r\n\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    transition: all 0.2s;\r\n\r\n    &--item {\r\n      margin: 2rem 0;\r\n    }\r\n\r\n    &--link {\r\n      color: $light;\r\n      font-size: 2.8rem;\r\n      font-weight: 400;\r\n      text-shadow: 0.1rem 0.1rem 0.4rem $secundario;\r\n      letter-spacing: 0.2rem;\r\n      transition: letter-spacing 0.2s;\r\n\r\n      &:hover {\r\n        letter-spacing: 0.4rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.clickHamburguer .navbar__cover {\r\n  transform: scale(125);\r\n\r\n  @media (min-width: $small) {\r\n    transform: scale(140);\r\n  }\r\n  @media (min-width: $medium) {\r\n    transform: scale(200);\r\n  }\r\n  @media (min-width: $large) {\r\n    transform: scale(250);\r\n  }\r\n  @media (min-width: $extra-large) {\r\n    transform: scale(300);\r\n  }\r\n}\r\n.clickHamburguer .navbar__list {\r\n  opacity: 1;\r\n  visibility: visible;\r\n}\r\n\r\n.line {\r\n  width: 3rem;\r\n  height: 0.4rem;\r\n  background-color: $light;\r\n  margin: 0.2rem 0;\r\n  transform-origin: right;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.clickHamburguer .line-1 {\r\n  transform: rotate(-45deg);\r\n}\r\n.clickHamburguer .line-2 {\r\n  opacity: 0;\r\n}\r\n.clickHamburguer .line-3 {\r\n  transform: rotate(45deg);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"12575e125015fd17b540008b20483acf","cacheData":{"env":{}}}